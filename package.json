{
  "name": "professor-genie-platform",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "prisma generate && next build",
    "build:local": "next build",
    "build:ext": "cross-env EXT_API_BASE_URL=https://api.profgenie.ai EXT_APP_BASE_URL=https://profgenie.ai node scripts/build.mjs",
    "build:ext:test": "cross-env EXT_API_BASE_URL=http://localhost:3000 EXT_APP_BASE_URL=http://localhost:3000 EXT_ENV=development node scripts/build.mjs",
    "start": "next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "migration:postgres:local": "./node_modules/.bin/dotenvx run -f .env.local -- pnpm dlx prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate"
  },
